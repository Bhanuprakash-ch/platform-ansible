#!/usr/bin/env python

from json import dumps
from re import match

output = {}
server_lines = [line for line in open('/etc/resolv.conf') if 'nameserver' in line]
server_ips = [match(r'^nameserver\s+([^\s]+)', line).group(1) for line in server_lines]

output['dns_servers'] = server_ips

print dumps(output)
